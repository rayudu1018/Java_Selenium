package Java;

public class SecondHighestNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int[] num = {1,2,3,4,5,6,7};
		int num1 = num[0];
		
		for(int i = 0;i<num.length;i++) {
			
		
		
		if (num[i] < num1) {
		
			num1 = num[i];
		}
		
		}System.out.println();
		
	}

}
