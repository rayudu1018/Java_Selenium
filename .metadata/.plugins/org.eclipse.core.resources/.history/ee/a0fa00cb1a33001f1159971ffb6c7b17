package Java2;

 abstract class AbstarctClass {
	// abstract String name;
	
	public abstract void subclass();
	
	public abstract void justclass();
	
    public void normalclass() {
    	
    	System.out.println("this class is from Abstrsct class like ** normal class method**");
    	
    }
	

}

 class normalclass extends AbstarctClass {
	
	public  void subclass() {
		System.out.println("my first abstract class");
	}
	
	public  void justclass() {
		System.out.println("my first abstract just class");
	}
	
	public  void class123() {
		System.out.println("my first abstract just class");
	}
	
}
 
 
 
 
