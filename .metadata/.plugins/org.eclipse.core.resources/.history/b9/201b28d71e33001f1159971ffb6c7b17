package Java2;

public abstract class Animal {
	
	public abstract void sound();
	
	public abstract void legs();
	
	public abstract void color();
	
	public static void name() {
		System.out.println("nwedfghjkl,mnbgftyu");
	}
	
	
}

class dog extends Animal{
	
	public  void sound() {
		
	}

