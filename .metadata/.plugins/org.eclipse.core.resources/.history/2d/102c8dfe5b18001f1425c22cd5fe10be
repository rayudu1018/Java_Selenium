package Java;

public class namesinner {

	public String name() {
		
		String n = "Venkata Rayudu Adapa";
		
		String[] b = n.split(" ");
		
		String f = b[0];
		String e = b[1];
		String w = b[2].substring(0,1);
		
		String totalname = f + " " +e+ " " +w;
		System.out.println(totalname);
		
		
		return "";
		
	}
}
